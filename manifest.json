{
    "manifest_version": 2,
    "name": "Accessibility Extension",
    "version": "0.2",
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["content.js"]
        }
    ],
    "background": {
        "scripts": ["background.js"]
    },
    "browser_action": {
        "default_icon": "ComplianceSheriffLogo.png"
    }
}